{
  "prettier.trailingComma": "none",
  "workbench.colorCustomizations": {
    "statusBar.background": "#030b05",
    "statusBar.noFolderBackground": "#aa8f35",
    "statusBar.debuggingBackground": "#aa2d2d",
    "activityBar.background": "#0a1061",
    "sideBarSectionHeader.background": "#191cb3",
    "activityBar.activeBackground": "#0e3317",
    "activityBar.activeBorder": "#6f50cf",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#6f50cf",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.border": "#030b05",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0e3317",
    "titleBar.activeBackground": "#030b05",
