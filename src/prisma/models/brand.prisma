type Brand {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  name: String!
  slug: String!
  description: String!
  images: [Image!] @relation(name: "BrandImage", storage: Embedded)
  category: [Category!] @relation(name: "CategoryBrand", onDelete: SET_NULL)
  department: [Department!] @relation(name: "BrandDepartment", onDelete: SET_NULL)
  product: [Product!] @relation(name: "ProductBrand")
}
