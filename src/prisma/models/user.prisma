type User {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  username: String! @unique
  password: String!
  birthdate: DateTime
  email: String! @unique
  posts: [Feed!]! @relation(name: "LinkAuthor", onDelete: SET_NULL)
  liked: [Feed]! @relation(name: "UserLike", onDelete: SET_NULL)
}
