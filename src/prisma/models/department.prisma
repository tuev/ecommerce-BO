type Department {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  description: String!
  name: String!
  slug: String!
  brand: [Brand!]
    @relation(name: "BrandDepartment", link: INLINE, onDelete: SET_NULL)
  category: [Category!]
    @relation(name: "CategoryDepartment", link: INLINE, onDelete: SET_NULL)
  product: [Product!] @relation(name: "ProductDepartment", onDelete: SET_NULL)
  images: [Image!] @relation(name: "DepartmentImage", storage: Embedded)
}
