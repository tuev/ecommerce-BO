type Category {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  description: String!
  name: String!
  slug: String!
  images: [Image!] @relation(name: "CategoryImage", storage: Embedded)
  brand: [Brand!]
    @relation(name: "CategoryBrand", link: INLINE, onDelete: SET_NULL)
  department: [Department!]
    @relation(name: "CategoryDepartment", onDelete: SET_NULL)
  product: [Product!] @relation(name: "ProductCategory")
}
